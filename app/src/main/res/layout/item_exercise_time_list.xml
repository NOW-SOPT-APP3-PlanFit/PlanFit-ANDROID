<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="22dp"
    android:background="@drawable/item_time_list_custom"
    android:paddingStart="17dp"
    android:paddingTop="22dp"
    android:paddingBottom="18dp">

    <TextView
        android:id="@+id/exercise_list_time_title"
        style="@style/Typography.PlanFit.subtitle01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:text="짧게"
        android:textColor="@color/gray01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/exercise_list_time"
        style="@style/Typography.PlanFit.body01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:text="약 29분"
        android:textColor="@color/gray01"
        app:layout_constraintBottom_toBottomOf="@id/exercise_list_time_title"
        app:layout_constraintStart_toEndOf="@+id/exercise_list_time_title"
        app:layout_constraintTop_toTopOf="@id/exercise_list_time_title"

        />

</androidx.constraintlayout.widget.ConstraintLayout>